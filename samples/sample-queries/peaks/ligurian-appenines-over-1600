<osm-script output="json">
    <!-- search the area of the Ligurian Appenines -->
    <query type="area">
        <has-kv k="boundary" v="administrative"/>
        <has-kv k="admin_level" v="6"/>
        <has-kv k="name" regv="Pavia|Genova|Alessandria|Piacenza|La Spezia"/>
    </query>
    <print mode="body" order="quadtile"/>
    <!-- get all peaks in the area -->
    <query type="node">
        <area-query/>
        <has-kv k="natural" v="peak"/>
        <has-kv k="name"/>
        <has-kv k="ele" regv="[1]{1}[6-9]{1}[0-9]{2}"/>
    </query>
    <print mode="body" order="quadtile"/>
</osm-script>