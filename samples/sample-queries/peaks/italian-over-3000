<osm-script output="json">
    <!-- search the area of the Dolmites -->
    <query type="area">
        <has-kv k="admin_level" v="2"/>
        <has-kv k="ISO3166-1" regv="^IT"/>
    </query>
    <print mode="meta" order="quadtile"/>
    <!-- get all peaks in the area -->
    <query type="node">
        <area-query/>
        <has-kv k="natural" v="peak"/>
        <has-kv k="name"/>
        <has-kv k="ele" regv="[3-4]{1}[0-9]{3}"/>
    </query>
    <print mode="body" order="quadtile"/>
</osm-script>